package org.eda1.actividad03.BSTree;

public class PruebasMain {
	
	private static BSTree<Integer> tree;

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
			Integer[] arr = {120, 87, 43, 65, 140, 99, 17, 130, 22, 150, 56, 100, 125, 145, 135};	    
			tree = new BSTree<Integer>();

			for (int i = 0;i < arr.length; i++)
	 			tree.add(arr[i]);
			
			System.out.println(tree.height());
			System.out.println(tree.numberOfLeaves());
			System.out.println(tree.toString());
			System.out.println(tree.findMin());
			System.out.println(tree.findMinIterative());
			System.out.println(tree.findMax());
			System.out.println(tree.findMaxIterative());
			System.out.println(tree.toStringLevel(2));
		
		
		
		
		

}
}